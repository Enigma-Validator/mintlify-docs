{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "HypeDexer Docs",
  "description": "Developer documentation for the HypeDexer Indexer API with EU & JP regional endpoints.",
  "colors": {
    "primary": "#00E5FF",
    "light": "#67E8F9",
    "dark": "#0891B2"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      { "label": "Website", "href": "https://www.hypedexer.com" }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://dashboard.hypedexer.com"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "auth",
              "regions",
              "best-practices"
            ]
          },
          {
            "group": "Usage & limits",
            "pages": [
              "rate-limits",
              "quotas",
              "errors",
              "changelog"
            ]
          },
          {
            "group": "SDKs & examples",
            "pages": [
              "sdk/typescript",
              "sdk/python",
              "examples"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "EU (Frankfurt)",
            "openapi": "/openapi-eu.yaml"
          },
          {
            "group": "JP (Tokyo)",
            "openapi": "/openapi-jp.yaml"
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        { "anchor": "EU Endpoint", "href": "https://api-eu.hypedexer.com", "icon": "globe" },
        { "anchor": "JP Endpoint", "href": "https://api-jp.hypedexer.com", "icon": "globe" },
        { "anchor": "Status", "href": "https://status.hypedexer.com", "icon": "activity" }
      ]
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "seo": {
    "indexing": "navigable",
    "metatags": {
      "og:title": "HypeDexer Docs",
      "og:description": "Indexer API docs with EU & JP endpoints, auth, limits and examples."
    }
  },
  "search": {
    "prompt": "Search HypeDexer docs and endpointsâ€¦"
  },
  "footer": {
    "socials": {
      "x": "https://x.com/EnigmaValidator",
      "github": "https://github.com/",
      "discord": "https://discord.gg/"
    }
  }
}
